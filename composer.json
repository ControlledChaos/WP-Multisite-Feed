{
  "name": "inpsyde/wp-multisite-feed",
  "description": "Create a separate feed for your whole multisite. This feed will have a custom url. You can limit the number of entries per blog and the maximum number of entries for the whole feed.",
  "keywords": [
    "rss",
    "feed",
    "international",
    "multi",
    "multisite",
    "network"
  ],
  "minimum-stability": "stable",
  "license": "GPL-3.0-or-later",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Inpsyde GmbH",
      "homepage": "https://github.com/inpsyde"
    },
    {
      "name": "Eric Teubert",
      "homepage": "https://github.com/eteubert"
    },
    {
      "name": "Frank Bueltge",
      "homepage": "https://github.com/bueltge"
    },
    {
      "name": "Moritz MeiÃŸelbach",
      "homepage": "https://github.com/Biont"
    }
  ],
  "require": {
     "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^6.3",
    "inpsyde/monkery-test-case": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Inpsyde\\MultisiteFeed\\": "inc/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Inpsyde\\MultisiteFeed\\Test\\Helper\\": "tests/Helper/",
      "": "tests/Stubs/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
